<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    >
    <com.player.mothercollege.view.MyUpDownListView
        android:id="@+id/lv_unity_fast"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:cacheColorHint="@color/transprant"
        android:divider="#0000"
        android:dividerHeight="0.5dp"
        android:listSelector="@color/transprant"/>
    <ImageView
        android:id="@+id/iv_fast_public"
        android:layout_width="53dp"
        android:layout_height="53dp"
        android:src="@mipmap/icon_consult_publish"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="29dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="31dp" />

</RelativeLayout>
